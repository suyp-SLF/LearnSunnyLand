#include <metal_stdlib>
#include <simd/simd.h>

using namespace metal;

struct main0_out
{
    float4 outColor [[color(0)]];
};

struct main0_in
{
    float2 inUV [[user(locn0)]];
};

fragment main0_out main0(main0_in in [[stage_in]], texture2d<float> texSampler [[texture(0)]], sampler texSamplerSmplr [[sampler(0)]])
{
    main0_out out = {};
    out.outColor = texSampler.sample(texSamplerSmplr, in.inUV);
    return out;
}

